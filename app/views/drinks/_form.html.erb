<%= simple_form_for(@drink) do |f| %>
  <%= f.input :name %>
  <%= f.input :brand %>
  <%= f.input :description %>
  <%= f.input :volume %>
  <%= f.input :caffeine %>
  <%= f.input :calories %>
  <%= f.label :avatar, 'Add Picture' %>
  <%= f.file_field :avatar %>
  <%= f.button :submit %>
<% end %>
